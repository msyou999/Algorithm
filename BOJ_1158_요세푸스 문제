#include <iostream>
#include <queue>
using namespace std;


int main() {
	int k;
	int n;
	queue<int> que;

	cin >> n >> k;

	for (int i = 1; n >= i; i++) {
		que.push(i);
	}

	cout << "<";
	while (!que.empty()) {
		for (int i = 0; i < k - 1; i++) {
			que.push(que.front());
			que.pop();
		}
		if (que.size() != 1)
			cout << que.front() << ", ";
		else
			cout << que.front();
		que.pop();
	}
	cout << ">";
	return 0;
}
