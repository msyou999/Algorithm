#include <cstdio>
#include <stack>
#include <vector>
using namespace std;

int main() {
	stack<int> st;
	vector<char> v;
	int i = 1;
	scanf("%d", &i);
	int index = 1;
	while(i--){
		int num;
		scanf("%d", &num);
		if (st.empty()) {
			st.push(index);
			index += 1;
			v.push_back('+');
		}
		if (!st.empty() && st.top() > num) {
			printf("NO");
			return 0;
		}
		while (!st.empty() && st.top() < num) {
			st.push(index);
			index += 1;
			v.push_back('+');
		}
		if (!st.empty() && st.top() == num) {
			st.pop();
			v.push_back('-');
		}
		
	}
	for (int k = 0; k < v.size(); k++) {
		printf("%c\n", v[k]);
	}
	return 0;

}
